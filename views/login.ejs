			<div class="main-logo">Keep in Mind</div>
		</section>
		<section class="controls">
			<a href="#" class="login-button">Login</a>
		</section>
		<section class="dropdown registration-dropdown">
			<form action="/login" class="login-form">
				<label class="form-line">Email</label>
